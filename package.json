{
  "name": "magergram",
  "version": "1.0.0",
  "description": "Instagram clone with NodeJS, Prisma, GraphQL, React and React Native",
  "repository": "https://github.com/magersoft/magergram.git",
  "author": "magersoft <magervlad@yandex.ru>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec babel-node src/server.js",
    "generate": "prisma generate",
    "prisma": "yarn run prisma:deploy && yarn run generate",
    "prisma:deploy": "prisma deploy --force",
    "predeploy": "yarn run generate",
    "deploy": "git push heroku master",
    "build": "babel src -d build",
    "postbuild": "cd src && copy ./api/*.graphql ../build/ && copy ./api/**/*.graphql ../build/ && copy ./api/**/**/*.graphql ../build/",
    "start": "node build/server.js"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "dotenv": "^8.2.0",
    "graphql-yoga": "^1.18.3",
    "graphql-tools": "^4.0.7",
    "graphql-upload": "^10.0.0",
    "is_js": "^0.9.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.3",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prisma-client-lib": "^1.34.10",
    "sharp": "^0.25.2",
    "twilio": "^3.40.0",
    "merge-graphql-schemas": "^1.7.6",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "copy": "^0.3.2",
    "nodemon": "^2.0.2"
  }
}
